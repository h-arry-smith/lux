#red {
  color: rgb(100, 0, 0);
}

#green {
  color: rgb(0, 100, 0);
}

#blue {
  color: rgb(0, 0, 100);
}

@delay 3s @fade 10s {
  [101->106] {
     color: cycle(0.2, #red, #green, #blue);
  }
}

@delay 13s {
  [101->106] {
    color: rgb(sin(), sin(), 0);
  }
}

@delay 20s {
  [101->106] {
    color: cycle(0.2, #red, #green);
  }
}