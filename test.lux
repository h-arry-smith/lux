#red {
  color: rgb(100, 0, 0);
}

#green {
  color: rgb(0, 100, 0);
}

#blue {
  color: rgb(0, 0, 100);
}

#low {
  intensity: 20;
}

#high {
  intensity: 70;
}

@fade 3s {
  [101->106] {
    intensity: cycle(1s, #low, #high);
  }
}

@delay 3s {
  [101->106] {
    color: #red;
  }
}

@delay 6s {
  [101->106] {
    color: #blue;
  }
}